<script>
  if (typeof Bugsnag !== 'undefined') {
    var online =  /help\.sharesight\.com/.exec(location.hostname);
    var stage = /^([^\-]{1,})\-help\.sharesight\.com/.exec(location.hostname);
    var env = online && (stage && stage[1] || 'production') || 'development';

    Bugsnag.releaseStage = <% if env_name %>'<%= env_name %>'<% else %>env<% end %>;
  }
</script>
